{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["last 2 versions", "ie >= 9"]
				}
			}
		],
		[
			"@babel/preset-react",
			{
				"pragma": "wp.element.createElement"
			}
		]
	],
	"plugins": [
		[
			"@wordpress/babel-plugin-makepot",
			{
				"output": "languages/formidable-js.pot"
			}
		]
	],
	"env": {
		"test": {
			"plugins": [
				"@babel/plugin-transform-react-jsx",
				"babel-plugin-styled-components"
			]
		}
	},
	"comments": true
}
